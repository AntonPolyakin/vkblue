{
    "plugins": ["react", "prettier"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2020,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "webextensions": true,
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended", "prettier", "prettier/react"],
    "rules": {
        "prettier/prettier": "error",
        "no-useless-constructor": "error",
        "no-console": 0,
        "react/prop-types": 0
    }
}
